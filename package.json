{
  "name": "svnerrorlookup",
  "description": "SVN error lookup",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Themacprod/SvnErrorLookup.git"
  },
  "private": true,
  "scripts": {
    "sass": "node-sass --include-path sass sass/index.sass public/bundle.css",
    "browserify": "browserify react_components/boot.js -t envify -o public/bundle.js",
    "uglifyjs": "uglifyjs public/bundle.js -m -o public/bundle.js",
    "cleancss": "cleancss -o public/bundle.css public/bundle.css",
    "build": "npm run browserify && npm run sass",
    "release": "npm run build && npm run uglifyjs && npm run cleancss",
    "postinstall": "npm run release",
    "start": "node index.js"
  },
  "contributors": [
    "Mathieu Albrespy <malbresp@matrox.com>"
  ],
  "license": "Proprietary",
  "dependencies": {
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "browserify": "^16.2.3",
    "child-process-promise": "^2.2.1",
    "child_process": "^1.0.2",
    "clean-css": "^3.4.19",
    "co": "^4.6.0",
    "codemirror": "^5.40.2",
    "cookie-parser": "^1.4.1",
    "create-react-class": "^15.6.3",
    "cron": "^1.4.1",
    "cross-spawn": "^6.0.5",
    "envify": "^4.1.0",
    "express": "^4.16.4",
    "express-session": "^1.13.0",
    "history": "^4.7.2",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "mongodb": "^3.1.8",
    "node-sass": "^4.9.4",
    "react": "15.6.2",
    "react-bootstrap": "^0.30.3",
    "react-codemirror2": "^4.3.0",
    "react-dom": "15.6.2",
    "react-router": "^2.8.1",
    "react-router-dom": "^4.3.1",
    "react-spinners": "^0.3.3",
    "serve-favicon": "^2.5.0",
    "ssh2": "^0.6.1",
    "superagent": "^3.8.3",
    "uglify-es": "^3.3.10",
    "xhr2": "^0.1.4"
  },
  "devDependencies": {
    "ajv": "^6.5.4",
    "ajv-keywords": "^3.2.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "jshint": "^2.9.6",
    "popper.js": "^1.14.4"
  }
}
